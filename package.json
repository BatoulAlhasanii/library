{
  "name": "app",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "ts-node-dev src/index.ts",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "db:seed": "ts-node src/database/seeders/index.ts",
    "typeorm": "ts-node ./node_modules/typeorm/cli.js",
    "typeorm:cli": "ts-node ./node_modules/typeorm/cli -f ./src/util/data-source.ts",
    "migration:run": "npm run typeorm migration:run",
    "migration:revert": "npm run typeorm migration:revert",
    "migration:generate": "./node_modules/.bin/ts-node -r tsconfig-paths/register ./node_modules/.bin/typeorm migration:generate -d src/util/data-source.ts src/database/migrations/migration",
    "test": "NODE_ENV=test  jest"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/express": "^4.17.17",
    "@types/sequelize": "^4.28.15",
    "bcrypt": "^5.1.0",
    "body-parser": "^1.20.2",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "express-rate-limit": "^6.9.0",
    "express-validator": "^7.0.1",
    "jsonwebtoken": "^9.0.1",
    "mysql2": "^3.6.0",
    "path": "^0.12.7",
    "reflect-metadata": "^0.1.13",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typeorm": "^0.3.19",
    "typeorm-extension": "^3.0.1",
    "typescript": "^5.1.6"
  },
  "devDependencies": {
